<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>PHP Boilerplate</title>
    <parser>
        <target>doc</target>
        <visibility>private</visibility>
    </parser>
    <transformer>
        <target>doc</target>
    </transformer>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>src</directory>
    </files>
</phpdoc>